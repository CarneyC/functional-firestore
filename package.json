    "@types/chai-uuid": "^1.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/ramda": "^0.26.39",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
