    "eslint-plugin-prettier": "^3.1.2",
    "fast-check": "^1.21.0",
    "husky": "^4.0.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.5.0",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.0",
    "sinon-chai": "^3.4.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
